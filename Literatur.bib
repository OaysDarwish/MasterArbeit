@Article{Frobenius1987,
  author   = {Frobenius, H. and Betzel, A.},
  journal  = {{Unfallchirurgie}},
  title    = {{Verletzungen und deren Ursache bei Fahrradunf{\"a}llen}},
  year     = {1987},
  issn     = {1615-3146},
  month    = {May},
  note     = {Aufgerufen am 28.06.2019},
  number   = {3},
  pages    = {135--141},
  volume   = {13},
  abstract = {W{\"a}hrend des Sommerhalbjahres 1984 wurden 356 Fahrradunf{\"a}lle erfa{\ss}t (73{\%} Erwachsene, 27{\%} Kinder) und aufgeschl{\"u}sselt nach Verletzungsart, Verletzungsschwere und nach Unfallursachen. Unter 594 Verletzungen {\"u}berwogen jene an der oberen Extremit{\"a}t, gefolgt von den zumeist schweren Kopfverletzungen. Die h{\"a}ufigste Verletzung ist die Klavikulafraktur vor der Radiusk{\"o}pfchenfraktur und der distalen Radiusfraktur bei Kindern. 59{\%} der Unf{\"a}lle ereigneten sich ohne Fremdeinwirkung, 58{\%} waren von den Radfahrem selbst verschuldet.},
  comment  = {Einleitung Der Umweltschutzgedanke, die Suche nach einer Alternative zum Auto sowie der Wunsch nach körperlicher Bewegung haben den Bestand an Fahrrädern in der Bundesrepublik Deutschland innerhalb der letzten 15 Jahre verdoppelt [3]. Aus verkehrspolitischer Sicht wird das Fahrrad innerhalb des Kurzstreckenverkehrs von etwa 5km in letzter Zeit immer wichtiger [3]. Andererseits steigt die Zahl der Fahrradunfälle von Jahr zu Jahr. So starben 1982 in der Bundesrepublik Deutschland fiber 1000 Fahrradfahrer im Stragenverkehr. Bisher gibt es keine Untersuchungen über typische Verletzungen und deren Häufigkeit bei diesen Verkehrsteilnehmem. Lediglich einzelne bekannte Verletzungsarten wurden isoliert herausgegriffen und analysiert, wie das stumpfe Bauchtrauma dutch den Fahrradlenker bei Kindern [5]. Ziel dieser Untersuchung war es, zu klären, ob es für Fahrradfahrer typische Verletzungen gibt. Naheliegend war auch, die Unfallursachen gteichzeitig mit zu erfassen, um Aufschlüsse über die Unfallsituationen und das Verhalten der Radfahrer zu erhalten.},
  day      = {01},
  doi      = {10.1007/BF02588417},
  url      = {https://doi.org/10.1007/BF02588417},
}

